
@{
    ViewBag.Title = "On-board Doctors - via Excel";
}

<h4>@ViewBag.Title</h4>
    <hr />
    @using (Html.BeginForm("Index", "BulkData", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <div class="form-horizontal">
            <div class="form-group">
                <div class="control-label col-md-2">Select file to upload:</div>
                <div class="col-md-10">
                    <input type="file" id="FileUpload" name="FileUpload" class="" />
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Upload" id="btnSubmit" class="btn btn-primary" />
                </div>
            </div>
        </div>
    }  