@using DPTS.Web.Models
@model AddressViewModel
@{
    ViewBag.Title = "Address Add";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("AccountNavigation")
<div class="col-lg-9 col-md-9 col-sm-8 col-xs-12 pull-right">
@using (Html.BeginForm("AddressAdd", "Doctor",
                                    new { },
                                    FormMethod.Post, new { @class = "tg-formeditprofile tg-haslayout do-account-setitngs" }))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(model => model.Id)

    @Html.Partial("_CreateUpdateAddress",Model)

    <button type="submit" class="tg-btn process-account-settings">Add</button>
}
</div>
