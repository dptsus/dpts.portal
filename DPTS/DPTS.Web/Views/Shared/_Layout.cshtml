@using DPTS.Web.Helpers
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - Find Doctors, Diagnostic Labs</title>
    <meta name="description" content="Find &amp; book appointments with doctors, diagnostic labs,  spas. Ask free health questions to doctors or get free tips from health experts" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="~/Scripts/jquery-1.9.1.min.js"></script>
    @Styles.Render("~/Content/wp-content/themes")
    @Scripts.Render("~/bundles/modernizr")
    @Html.Assets().Styles.Render()
</head>
<body class="home page page-id-2 page-template-default home  sample-page mec-home ">
    @*<div class="preloader-outer">
            <div class="pin"></div>
            <div class="pulse"></div>
        </div>*@
    <div id="wrapper" class="tg-haslayout">
        @Html.Partial("_Header")
        @RenderBody()
        @Html.Partial("_Footer")
    </div>
    <div id="loader" class="overlay">
        <div class="load-container load1">
            <div class="loader">Loading... </div>
        </div>
    </div>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("Scripts", required: false)
    <script src="~/Scripts/jquery.ui.widget.js"></script>
    @*<script src="~/Scripts/docdirect_functions.js"></script>*@
    @Html.Assets().Scripts.Render()
</body>
</html>
